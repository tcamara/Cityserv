<div class="contact_list">
  <table cellpadding="0" cellspacing="0">
      <tr>
        <th></th>
        <th>
          Name
        </th>
        <th>
          Company
        </th>
        <th>
          Job
        </th>
        <th>
          Phone
        </th>
        <th>
          Email
        </th>
        <th>
          Address
        </th>
        <th> </th>

      </tr>
      <% coll.each do |i| %>
      <tr class="<%= cycle("even","odd") %>">
        <td class="noborder"><%= link_to 'View', contact_path(i.id) %></td>
        <td><%= i.first_name %> <%= i.last_name %></td>
        <td><%= i.company_name %></td>
        <td><%= i.job_title %></td>
        <td><%= i.work_phone %></td>
        <td><a href="mailto:<%= i.email.to_s %>" class="email_icon"><%= image_tag "email.png" %></a><%= i.email %></td>
        <td><%= i.company_address %></td>
        <td>
          <%= link_to image_tag("delete.png"), { :controller => "associateds", :action => "delete", :cid => i.id, :pid => @property.id, :ctype => ctype }, :confirm => "Are you sure?", :method => :delete %>
        </td>
      </tr>
      <% end %>
  </table>
</div>